pragma solidity >= 0.5.0 < 0.7.0;
contract insuranceprediction{
 
struct policyrequest{

	// State variables
	
	uint policyrequest_id;
	uint agent_id;
	uint policy_id;
	string vechiclenum;
	string modelnum;
	string enginenum;
	string date;
	string status;

}

 
policyrequest []m;



function acceptedrequest(uint policyrequest_id,uint agent_id,uint policy_id, string memory vechiclenum,string memory modelnum,string memory enginenum,string memory date,string memory status) public{
	policyrequest memory m1=policyrequest(policyrequest_id,agent_id,policy_id,vechiclenum,modelnum,enginenum,date,status);
	m.push(m1);
}


}
